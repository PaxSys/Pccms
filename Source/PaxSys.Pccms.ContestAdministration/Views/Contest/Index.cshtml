@model PaxSys.Pccms.ContestAdministration.Models.ContestViewModel[]

@Html.ActionLink("Create New Contest", "AddContest", new {}, new {@class = "btn btn-sm btn-info"})

<table class="table table-striped">
    <thead>
    <tr>
        <th>Id</th>
        <th>Description</th>
        <th>ContestDate</th>
        <th>IsOver</th>
        <th></th>
    </tr>
    </thead>
    <tbody>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Description</td>
            <td>@item.Date</td>
            <td>@item.IsOver</td>
            <td>
                @Html.ActionLink("Details", "Details",
                    new { id = item.Id },
                    new {@class = "btn btn-sm btn-info"})
            </td>
        </tr>
    }
    </tbody>
</table>
